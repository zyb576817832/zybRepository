package com.ebon.rpc.sap.exception;

public class SapException extends Exception {

	private static final long serialVersionUID = -7654266417881560342L;

	public SapException() {
		super();
	}

	/**
	 * @param message
	 */
	public SapException(String message) {
		super(message);
	}

	/**
	 * @param message
	 * @param cause
	 */
	public SapException(String message, Throwable cause) {
		super(message, cause);
	}

	/**
	 * @param cause
	 */
	public SapException(Throwable cause) {
		super(cause);
	}
}
