package com.ebon.rpc.project.vo;

import java.io.Serializable;

public class ProjectResponse implements Serializable {
	
	private static final long serialVersionUID = 6102071535276768571L;
	
	private String isOk; // 是否成功标识
	private String errorCode; // 错误代码
	private String errorMessage;// 错误信息
	private ProjectInfo[] ProjectInfos;// 项目信息

	public String getIsOk() {
		return isOk;
	}

	public void setIsOk(String isOk) {
		this.isOk = isOk;
	}

	public String getErrorCode() {
		return errorCode;
	}

	public void setErrorCode(String errorCode) {
		this.errorCode = errorCode;
	}

	public String getErrorMessage() {
		return errorMessage;
	}

	public void setErrorMessage(String errorMessage) {
		this.errorMessage = errorMessage;
	}

	public ProjectInfo[] getProjectInfos() {
		return ProjectInfos;
	}

	public void setProjectInfos(ProjectInfo[] projectInfos) {
		ProjectInfos = projectInfos;
	}

}
