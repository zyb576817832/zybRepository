package com.ebon.rpc.opl.vo;

import java.io.Serializable;

public class OplListResponse implements Serializable{

	private static final long serialVersionUID = -6932449490062156181L;

	private String isOk;//是否成功标识
	private String errorCode;//错误代码
	private String errorMessage;//错误信息
	private OplInfo[] oplInfos;//OPL信息
	
	public String getIsOk() {
		return isOk;
	}
	public void setIsOk(String isOk) {
		this.isOk = isOk;
	}
	public String getErrorCode() {
		return errorCode;
	}
	public void setErrorCode(String errorCode) {
		this.errorCode = errorCode;
	}
	public String getErrorMessage() {
		return errorMessage;
	}
	public void setErrorMessage(String errorMessage) {
		this.errorMessage = errorMessage;
	}
	public OplInfo[] getOplInfos() {
		return oplInfos;
	}
	public void setOplInfos(OplInfo[] oplInfos) {
		this.oplInfos = oplInfos;
	}
	
}
