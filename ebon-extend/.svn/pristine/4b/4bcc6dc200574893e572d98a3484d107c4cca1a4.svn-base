	private boolean isOverheadAndBlank(String s) {
		return (s == null || s.trim().equals("")) && this.isOverhead7_V();
	}

	private boolean isOverhead7_V() {
		return this.projectCode.startsWith("SUP/007")
				|| this.projectCode.startsWith("SUP/008")
				|| this.projectCode.startsWith("SUP/009")
				|| this.projectCode.startsWith("SUP/010")
				|| this.projectCode.startsWith("SUP/011")
				|| this.projectCode.startsWith("SUP/012")
				|| this.projectCode.equalsIgnoreCase("VAC/001");
	}

