package com.ebon.rpc.opl.vo;

import java.io.Serializable;

public class OplInsertRequest implements Serializable {

	private static final long serialVersionUID = -7599865069265976173L;
	
	private String systemSource;
	
	private OplInfo[] oplInfos;

	public String getSystemSource() {
		return systemSource;
	}

	public void setSystemSource(String systemSource) {
		this.systemSource = systemSource;
	}

	public OplInfo[] getOplInfos() {
		return oplInfos;
	}

	public void setOplInfos(OplInfo[] oplInfos) {
		this.oplInfos = oplInfos;
	} 

}
